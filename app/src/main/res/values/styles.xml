<?xml version="1.0" encoding="utf-8"?>

<resources>
    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="queryBackground">@android:color/transparent</item>
        <item name="submitBackground">@android:color/transparent</item>
        <item name="searchHintIcon">@android:color/transparent</item>
        <item name="searchIcon">@android:color/transparent</item>
        <item name="background">@android:color/transparent</item>
    </style>
    <style name="name">
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#DE000000</item>
    </style>
    <style name="label">
        <item name="android:textSize">20sp</item>
        <item name="android:letterSpacing">0.01</item>
        <item name="android:textColor">#61000000</item>
    </style>
</resources>